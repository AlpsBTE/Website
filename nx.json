{
  "npmScope": "alpsbte",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "alpsbte-api": {
      "tags": ["scope:api", "type:api"]
    },
    "alpsbte-web": {
      "tags": ["scope:app", "type:app"]
    },
    "alpsbte-web-e2e": {
      "tags": [],
      "implicitDependencies": ["alpsbte-web"]
    },
    "ui": {
      "tags": ["scope:shared", "type;ui"]
    }
  }
}
