{
  "npmScope": "alpsbte",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "about-us": {
      "tags": ["scope:about-us", "type:feature"]
    },
    "alpsbte-api": {
      "tags": ["scope:api", "type:api"]
    },
    "alpsbte-spikeCMS": {
      "tags": []
    },
    "alpsbte-web": {
      "tags": ["scope:app", "type:app"]
    },
    "application": {
      "tags": ["scope:application", "type:feature"]
    },
    "contact": {
      "tags": ["scope:contact", "type:feature"]
    },
    "downloads": {
      "tags": ["scope:downloads", "type:feature"]
    },
    "error": {
      "tags": ["scope:error", "type:feature"]
    },
    "faq": {
      "tags": ["scope:faq", "type:feature"]
    },
    "gallery": {
      "tags": ["scope:gallery", "type:feature"]
    },
    "home": {
      "tags": ["scope:home", "type:feature"]
    },
    "loader": {
      "tags": ["scope:loader", "type:feature"]
    },
    "router": {
      "tags": ["sccope:router", "type:feature"]
    },
    "shared-components": {
      "tags": ["scope:shared-components", "type:ui"]
    },
    "shared-config": {
      "tags": []
    },
    "shared-language": {
      "tags": ["scope:shared-language", "type:feature"]
    },
    "shared-stores": {
      "tags": ["scope:shared-stores", "type:feature"]
    },
    "shared-theme": {
      "tags": ["scope:shared-theme", "type:ui"]
    },
    "shared-util": {
      "tags": ["scope:shared-util", "type:feature"]
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "alpsbte-web"
}
